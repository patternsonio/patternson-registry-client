        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "aws-appsync": "1.7.1",
    "aws-sdk": "2.396.0",
    "babel-runtime": "6.26.0",
    "isomorphic-fetch": "2.2.1",
    "url-join": "4.0.0",
    "ws": "^3.3.3"
  }
